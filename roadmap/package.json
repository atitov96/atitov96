{
  "name": "roadmap",
  "version": "0.1.0",
  "homepage": "/roadmap",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && touch out/.nojekyll",
    "postbuild": "cp out/.nojekyll ../dist/roadmap/",
    "start": "next start",
    "lint": "next lint",
    "serve:base": "mkdir -p temp/roadmap && cp -r out/* temp/roadmap/ && npx serve temp -p 3000"
  },
  "dependencies": {
    "@types/dagre": "^0.7.52",
    "axios": "^1.7.9",
    "dagre": "^0.8.5",
    "framer-motion": "^11.15.0",
    "gh-pages": "^5.0.0",
    "lucide-react": "^0.469.0",
    "next": "15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
